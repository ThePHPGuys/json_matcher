{
    "name": "fesor/json_matcher",
    "description": "Collection if JSON matchers for painless usage",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Protko",
            "email": "fesors@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
      "psr-4": {"Fesor\\JsonMatcher\\": "src/"}
    },
    "autoload-dev": {
      "psr-4": {"spec\\Fesor\\JsonMatcher\\": "spec/"}
    },
    "require": {
      "php": ">=5.4.0"
    },
    "require-dev": {
      "phpspec/phpspec": "^3.3",
      "leanphp/phpspec-code-coverage": "^3.1",
      "friendsofphp/php-cs-fixer": "^2.3"
    },
    "config": {
      "bin-dir": "bin"
    }
}
